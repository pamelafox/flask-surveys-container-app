{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "name": {
        "value": "${SERVICE_WEB_NAME}"
      },
      "imageName": {
        "value": "${SERVICE_WEB_IMAGE_NAME}"
      },
      "postgresDomainName": {
        "value": "${POSTGRES_DOMAIN_NAME}"
      },
      "postgresUser": {
        "value": "${POSTGRES_USER}"
      },
      "postgresDatabaseName": {
        "value": "${POSTGRES_DATABASE_NAME}"
      },
      "postgresPassword": {
        "reference": {
          "keyVault": {
            "id": "/subscriptions/32ea8a26-5b40-4838-b6cb-be5c89a57c16/resourceGroups/flasksurveys-rg/providers/Microsoft.KeyVault/vaults/flasksurveys-xvmc-vault"
          },
